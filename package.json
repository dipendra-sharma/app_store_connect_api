{
  "name": "@dipendra/app-store-connect-api",
  "version": "1.0.0",
  "description": "A TypeScript library for interacting with the Apple App Store Connect API. Generated using OpenAPI Generator, this package provides strongly-typed client for App Store Connect API integration.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "generate": "npx openapi-generator-cli generate -i specification/app_store_connect_api_openapi.json -g typescript-fetch -o .",
    "build": "tsc",
    "compile": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "author": "Dipendra Sharma",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "keywords": [
    "apple",
    "app-store",
    "app-store-connect",
    "api",
    "typescript",
    "openapi"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/app-store-connect-api"
  }
}
